alias podversion='cat *.podspec | grep s.version '
alias nano='echo so eine scheisse gibt es hier nicht!'
export PATH="$HOME/.fastlane/bin:$PATH"
alias clearxcode='rm -rf ~/Library/Developer/Xcode/DerivedData/*'
alias clearpods='rm -rf ~/Library/Caches/CocoaPods;pod deintegrate; pod setup; pod install;'
alias printdeps='pod dependencies --graphviz --image'
